CREATE TABLE funcionarios (id int, nome varchar(255), setor int, cargo varchar(255), salario decimal)

-- TABELA: Funcionários
-- Colunas:
-- codigo INT
-- Nome VARCHAR (255)
-- Setor INT
-- Cargo VARCHAR (255)
-- Salario DECIMAL

